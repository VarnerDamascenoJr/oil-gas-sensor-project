datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model SensorData {
  id          Int      @id @default(autoincrement())
  equipmentId String
  timestamp   DateTime
  value       Float
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}
